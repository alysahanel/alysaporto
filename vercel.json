{
  "version": 2,
  "builds": [
    {
      "src": "gams-01/api/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "gams-01/public/**"
        ]
      }
    },
    {
      "src": "legal-web/api/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "legal-web/frontend/public/**",
          "legal-web/backend/public-assets/**"
        ]
      }
    },
    {
      "src": "CashTracking/backend/api/index.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source": "/gams/api/:match*",
      "destination": "/gams-01/api/index.js"
    },
    {
      "source": "/gams/:match*",
      "destination": "/gams-01/api/index.js"
    },
    {
      "source": "/legal/api/:match*",
      "destination": "/legal-web/api/index.js"
    },
    {
      "source": "/legal/:match*",
      "destination": "/legal-web/api/index.js"
    },
    {
      "source": "/cashtracking/static/:match*",
      "destination": "/CashTracking/frontend/build/static/:match*"
    },
    {
      "source": "/cashtracking/manifest.json",
      "destination": "/CashTracking/frontend/build/manifest.json"
    },
    {
      "source": "/cashtracking/favicon.ico",
      "destination": "/CashTracking/frontend/build/favicon.ico"
    },
    {
      "source": "/cashtracking/logo192.png",
      "destination": "/CashTracking/frontend/build/logo192.png"
    },
    {
      "source": "/cashtracking/api/:match*",
      "destination": "/CashTracking/backend/api/index.py"
    },
    {
      "source": "/cashtracking/:match*",
      "destination": "/CashTracking/frontend/build/index.html"
    }
  ]
}
